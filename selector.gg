data (select row_number() over () as id, * from '../data/metrics_project/actually.shp' limit 1000) econ
data (select count(*)::float / 1000 from econ where econ.LandValue between $amt.xmin and $amt.xmax) price_range 

dot
    x econ.LandValue
    y econ.BuildValue
    param amt x

text
    value price_range
